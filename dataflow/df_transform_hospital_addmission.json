{
	"name": "df_transform_hospital_addmission",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_hospital_addmission_raw",
						"type": "DatasetReference"
					},
					"name": "HospitalAddmissionSource"
				},
				{
					"dataset": {
						"referenceName": "ds_country_lookup",
						"type": "DatasetReference"
					},
					"name": "Countrysource"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_date",
						"type": "DatasetReference"
					},
					"name": "dimDateSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hospital_addmission_sink",
						"type": "DatasetReference"
					},
					"name": "sinkWeeklydata"
				},
				{
					"dataset": {
						"referenceName": "ds_hospital_addmission_sink",
						"type": "DatasetReference"
					},
					"name": "DailyDatasink"
				}
			],
			"transformations": [
				{
					"name": "selectRequiredFields"
				},
				{
					"name": "countrylookup"
				},
				{
					"name": "selectRequiredFields2"
				},
				{
					"name": "splitDailyFromWeekly"
				},
				{
					"name": "YearWeekColumn"
				},
				{
					"name": "aggregateDatebyweek"
				},
				{
					"name": "joinYearWeek"
				},
				{
					"name": "pivotweeklyaddmissionICUcases"
				},
				{
					"name": "PivotDailyData"
				},
				{
					"name": "sortweekly"
				},
				{
					"name": "sortDailyData"
				},
				{
					"name": "select1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          indicator as string,",
				"          date as date,",
				"          year_week as string,",
				"          value as integer,",
				"          source as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> HospitalAddmissionSource",
				"source(output(",
				"          country as string,",
				"          country_code_2_digit as string,",
				"          country_code_3_digit as string,",
				"          continent as string,",
				"          population as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Countrysource",
				"source(output(",
				"          date_key as string,",
				"          date as string,",
				"          year as string,",
				"          month as string,",
				"          day as string,",
				"          day_name as string,",
				"          day_of_year as string,",
				"          week_of_month as string,",
				"          week_of_year as string,",
				"          month_name as string,",
				"          year_month as string,",
				"          year_week as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> dimDateSource",
				"HospitalAddmissionSource select(mapColumn(",
				"          country,",
				"          indicator,",
				"          reported_date = date,",
				"          reported_year_week = year_week,",
				"          value,",
				"          source,",
				"          url",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectRequiredFields",
				"selectRequiredFields, Countrysource lookup(selectRequiredFields@country == Countrysource@country,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> countrylookup",
				"countrylookup select(mapColumn(",
				"          country = selectRequiredFields@country,",
				"          indicator,",
				"          reported_date,",
				"          reported_year_week,",
				"          value,",
				"          source,",
				"          url,",
				"          country = Countrysource@country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          population",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectRequiredFields2",
				"selectRequiredFields2 split(indicator == 'Weekly new hospital admissions per 100k' || indicator == 'Weekly new ICU admissions per 100k',",
				"     disjoint: false) ~> splitDailyFromWeekly@(Weekly, Daily)",
				"dimDateSource derive(ecdc_year_week = year+'-w'+ lpad(week_of_year, 2, '0')) ~> YearWeekColumn",
				"YearWeekColumn aggregate(groupBy(ecdc_year_week),",
				"     week_start_date = min(date),",
				"          week_end_date = max(date)) ~> aggregateDatebyweek",
				"splitDailyFromWeekly@Weekly, aggregateDatebyweek join(reported_year_week == ecdc_year_week,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinYearWeek",
				"joinYearWeek pivot(groupBy(country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          reported_year_week,",
				"          reported_date,",
				"          population,",
				"          source,",
				"          week_start_date,",
				"          week_end_date),",
				"     pivotBy(indicator, ['Weekly new hospital admissions per 100k', 'Weekly new ICU admissions per 100k']),",
				"     Count = sum(value),",
				"     columnNaming: '$N_$V',",
				"     lateral: true) ~> pivotweeklyaddmissionICUcases",
				"splitDailyFromWeekly@Daily pivot(groupBy(country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          reported_year_week,",
				"          reported_date,",
				"          population,",
				"          source),",
				"     pivotBy(indicator, ['Daily hospital occupancy', 'Daily ICU occupancy']),",
				"     count = sum(value),",
				"     columnNaming: '$N_$V',",
				"     lateral: true) ~> PivotDailyData",
				"pivotweeklyaddmissionICUcases sort(desc(reported_year_week, true),",
				"     asc(country, true)) ~> sortweekly",
				"PivotDailyData sort(desc(reported_date, true),",
				"     asc(country, true)) ~> sortDailyData",
				"sortweekly select(mapColumn(",
				"          country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          reported_year_week,",
				"          population,",
				"          source,",
				"          week_start_date,",
				"          week_end_date,",
				"          Count_Weekly_hospital_admissions_per_100k = {Count_Weekly new hospital admissions per 100k},",
				"          Count_Weekly_ICU_admissions_per_100k = {Count_Weekly new ICU admissions per 100k}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"sortDailyData select(mapColumn(",
				"          country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          reported_year_week,",
				"          reported_date,",
				"          population,",
				"          source,",
				"          count_Daily_hospital_occupancy = {count_Daily hospital occupancy},",
				"          count_Daily_ICU_occupancy = {count_Daily ICU occupancy}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkWeeklydata",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DailyDatasink"
			]
		}
	}
}